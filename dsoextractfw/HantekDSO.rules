# Hantek DSO-2090
SUBSYSTEM=="usb", ACTION=="add", ENV{DEVTYPE}=="usb_device", ENV{PRODUCT}=="4b4/2090/*", RUN+="/sbin/fxload -t fx2 -I /usr/share/HantekDSO/DSO2090_firmware.hex -s /usr/share/HantekDSO/DSO2090_loader.hex -D $env{DEVNAME}"

